{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-gd": "*",
        "ext-json": "*",
        "darryldecode/cart": "^4.1",
        "doctrine/dbal": "^2.10",
        "spatie/laravel-ignition": "^1.0",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "iyzico/iyzipay-php": "^2.0",
        "laravel-validation-rules/credit-card": "^1.2",
        "laravel/framework": "^9.0",
        "laravel/helpers": "^1.4",
        "laravel/horizon": "5.*",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "maatwebsite/excel": "^3.1",
        "murat/laravel-meta-tags": "dev-main",
        "spatie/laravel-backup": "^6.14",
        "unisharp/laravel-filemanager": "~1.9.2",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "fruitcake/laravel-telescope-toolbar": "^1.2",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers/helpers.php",
            "app/helpers/cartHelpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
